{%- if collections %}

<article id="students">
<h2>{{title}}</h2>
<ul class="testimonials masonry">
{%- for testimonial in collections.testimonials | reverse %}
    {%- set testimonialData = testimonial.data %}
    {%- set testimonialContent = testimonial.templateContent %}
    {%- include 'partials/testimonial.njk' %}
{%- endfor %}
</ul>
</article>
{% endif %}